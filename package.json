{
  "name": "el-web",
  "version": "1.0.0",
  "private": true,
  "description": "Just Another React Js Components Library.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist --ignore '**/*.test.js'",
    "build:watch": "babel src -w -d dist",
    "lint": "eslint src/**; exit 0",
    "lint:watch": "esw -w src/**",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "TEST=true jest --watchAll --silent",
    "test:coverage": "TEST=true jest --coverage --silent",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "updateSnapshots": "jest --updateSnapshot",
    "updateTests": "npm run test -- -u",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Medical Web Experts",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.14",
    "@storybook/addon-links": "^3.2.14",
    "@storybook/react": "^3.2.14",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "2.9.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-enzyme": "^3.8.3",
    "jest-serializer-enzyme": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.0.0",
    "styled-jsx": "^2.2.1",
    "react-jss": "8.2.1"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "prop-types": "^15.6.0",
    "react-dom": "^16.2.0",
    "styled-jsx": "^2.2.1"
  },
  "jest": {
    "rootDir": "./src",
    "setupFiles": [
      "../jest/polyfillSetup.js"
    ],
    "globals": {
      "__DEV__": true
    },
    "snapshotSerializers": [
      "../node_modules/jest-serializer-enzyme"
    ],
    "setupTestFrameworkScriptFile": "../node_modules/jest-enzyme/lib/index.js"
  }
}
