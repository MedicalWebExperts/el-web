{
  "name": "el-web",
  "version": "0.1.3",
  "description": "Just Another React Js Components Library.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist --ignore '**/*.test.js'",
    "build:watch": "babel src -w -d dist",
    "lint": "eslint src/**",
    "lint:watch": "esw -w src/**",
    "test": "jest",
    "test:watch": "TEST=true jest --watchAll --silent",
    "test:coverage": "TEST=true jest --coverage --silent",
    "updateTests": "npm run test -- -u",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "author": "Medical Web Experts",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "3.4.4",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-enzyme": "^6.0.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "styled-jsx": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "prop-types": "^15.6.2",
    "styled-jsx": "^3.0.2"
  },
  "jest": {
    "rootDir": "./src",
    "setupFiles": [
      "../jest/polyfillSetup.js"
    ],
    "globals": {
      "__DEV__": true
    },
    "snapshotSerializers": [
      "../node_modules/enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "../node_modules/jest-enzyme/lib/index.js"
  }
}
